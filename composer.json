{
  "name": "byjg/restserver",
  "description": "Create RESTFull services with different and customizable output handlers (JSON, XML, Html, etc.). Auto-Generate routes from swagger.json definition.",
  "autoload": {
    "psr-4": {
      "ByJG\\RestServer\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "byjg/serializer": "dev-major_refactory",
    "byjg/singleton-pattern": "1.0.*",
    "nikic/fast-route": "v1.2.*",
    "filp/whoops": "^2.14",
    "byjg/cache-engine": "4.0.*",
    "byjg/webrequest": "^5.0.0.x-dev",
    "ext-json": "*"
  },
  "suggest": {
    "ext-fileinfo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*|7.4.*|^9.5"
  },
  "license": "MIT"
}

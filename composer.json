{
  "name": "byjg/restserver",
  "description": "Create RESTFull services with different and customizable output handlers (JSON, XML, Html, etc.). Auto-Generate routes from swagger.json definition.",
  "autoload": {
    "psr-4": {
      "ByJG\\RestServer\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4",
    "byjg/serializer": "4.9.*",
    "byjg/singleton-pattern": "4.9.*",
    "nikic/fast-route": "v1.3.*",
    "filp/whoops": "^2.14",
    "byjg/cache-engine": "^4.9.x-dev",
    "byjg/webrequest": "4.9.*",
    "byjg/jwt-wrapper": "^4.9|^5.0",
    "psr/log": "^1.0|^2.0",
    "ext-json": "*"
  },
  "suggest": {
    "ext-fileinfo": "*"
  },
  "require-dev": {
    "monolog/monolog": "^2.9|^3.4",
    "phpunit/phpunit": "5.7.*|7.4.*|^9.6"
  },
  "license": "MIT"
}

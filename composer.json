{
  "name": "byjg/restserver",
  "description": "Create RESTFull services with different and customizable output handlers (JSON, XML, Html, etc.). Auto-Generate routes from swagger.json definition.",
  "authors": [
    {
      "name": "João Gilberto Magalhães",
      "email": "joao@byjg.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByJG\\RestServer\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.0",
    "byjg/serializer": "1.0.*",
    "byjg/singleton-pattern": "1.0.*",
    "nikic/fast-route": "v1.2.*",
    "filp/whoops": "2.1.*",
    "byjg/cache-engine": "4.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*|7.4.*"
  },
  "license": "MIT"
}

{
  "name": "byjg/restserver",
  "description": "Create RESTFull services with different and customizable output handlers (JSON, XML, Html, etc.). Auto-Generate routes from swagger.json definition.",
  "autoload": {
    "psr-4": {
      "ByJG\\RestServer\\": "src/"
    }
  },
    "autoload-dev": {
        "psr-4": {
        "Tests\\": "tests/"
        }
    },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "byjg/serializer": "^5.0",
    "byjg/singleton-pattern": "^5.0",
    "nikic/fast-route": "v1.3.*",
    "filp/whoops": "^2.14",
    "byjg/cache-engine": "^5.0",
    "byjg/webrequest": "dev-5.0.0rc",
    "byjg/jwt-wrapper": "^5.0",
    "ext-json": "*"
  },
  "suggest": {
    "ext-fileinfo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*|7.4.*|^9.6"
  },
  "license": "MIT"
}
